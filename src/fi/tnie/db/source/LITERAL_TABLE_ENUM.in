{{column-enum-list}}
			
	private ColumnName name;	
	private DataType type;
	private boolean definitelyNotNullable;
	private Boolean autoIncrementColumn;
				
	{{literal-table-enum}}(Identifier name, DataType type, boolean definitelyNotNullable, Boolean autoIncrementColumn) {
